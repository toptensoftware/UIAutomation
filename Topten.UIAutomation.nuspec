<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2013/05/nuspec.xsd">
  <metadata>
    <id>Topten.UIAutomation</id>
    <title>Topten.UIAutomation</title>
    <version>0.0.1</version>
    <authors>Topten Software</authors>
    <owners>Topten Software</owners>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <license type="expression">Apache-2.0</license>
    <licenseUrl>https://licenses.nuget.org/Apache-2.0</licenseUrl>
    <projectUrl>https://github.com/toptensoftware/UIAutomation</projectUrl>
    <description>UIAutomation References for .NET Framework and .NET Core</description>
    <releaseNotes>Nothing to see here :)</releaseNotes>
    <repository type="git" url="https://github.com/toptensoftware/UIAutomation" />
    
    <references>
      <group targetFramework="netcoreapp3.1">
        <reference file="UIAutomationClient.dll" />
        <reference file="UIAutomationClient.xml" />
        <reference file="UIAutomationProvider.dll" />
        <reference file="UIAutomationProvider.xml" />
        <reference file="UIAutomationTypes.dll" />
        <reference file="UIAutomationTypes.xml" />
        <reference file="WindowsBase.dll" />
        <reference file="WindowsBase.xml" />
      </group>
    </references>

    <dependencies>
      <group targetFramework=".NETCoreApp3.1">
      </group>
    </dependencies>

    <frameworkAssemblies>
      <frameworkAssembly assemblyName="UIAutomationClient" />
      <frameworkAssembly assemblyName="UIAutomationProvider" />
      <frameworkAssembly assemblyName="UIAutomationTypes" />
      <frameworkAssembly assemblyName="WindowsBase" />
    </frameworkAssemblies>

  </metadata>
  <files>
  
    <!-- netcoreapp3.1 Reference -->
    <file src= ".\ref\*" target="ref\netcoreapp3.1" exclude=".\ref\UIAutomationClientSide*.*"/>

    <!-- netcoreapp3.1 Library -->
    <file src= ".\ref\*" target="lib\netcoreapp3.1" exclude=".\ref\UIAutomationClientSide*.*" />

    <!-- netcoreapp3.1 Runtime -->
    <file src= ".\win-x64\*" target="runtimes\win-x64\lib\netcoreapp3.1" exclude=".\win-x64\UIAutomationClientSide*.*" />

  </files>
</package>